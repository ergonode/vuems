<%= options.store.map(({ fullname }) =>`import ${'store_' + hash(fullname)} from '${fullname.replace(/\\/g,'/')}';`).join('\n') %>

export default {
<%= options.store.map(({ fullname, path }) => {
    const storeName = path.split('/');

    return `\t${storeName[storeName.length - 1]}: store_${hash(fullname)},`;
}).join('\n') %>
};
